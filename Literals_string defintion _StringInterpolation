void main() {
  /* Literals 
  "john"
    2
    4.2
    */
  //Types of string defintion
  String lan="dart is also similar like other languages";
  String s1='it\'s also awesomme';
  String s2="it's also easy";
  print(lan);
  print(s1);
  print(s2);
  //String interpolation
  String name=":Ashwani";
  print("i want to learn dart $name");
  var l=10,b=20;
  print("Perimeter of rectangle $l and $b is ${2*(l+b)}");
  
}
